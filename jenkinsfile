pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
    
        stage('Build') {
            steps {
                echo 'Build Project'
            }
        }
    

        stage('Deploy') {
            steps {
                echo 'deploying'
            }
        }
    
   
        stage('Test') {
            steps {
                echo 'testing'
            }
        }

    
        stage('Release') {
            steps {
                echo 'Release'
            }
        }
    }
}
